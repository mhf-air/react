{
  "name": "react",
  "version": "1.0.0",
  "repository": "https://github.com/mhf-air/react.git",
  "author": "mhf <mhfcbz@163.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "watch": "watchify -vd -e src/app.js -o dist/app.js",
    "serve": "live-server dist --entry-file=index.html",
    "dev": "npm-run-all --parallel watch serve",
    "production": "NODE_ENV=\"production\" browserify -e src/app.js | uglifyjs -c -m > dist/app.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babelify": "^8.0.0",
    "browserify": "^16.0.0",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.3.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "uglify-js": "^3.3.10",
    "watchify": "^3.10.0"
  }
}
